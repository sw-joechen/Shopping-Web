{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run after-build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "after-build": "npm run rm-cssDir && npm run rm-jsDir && npm run rm-imgDir && mv -f dist/* ../",
    "rm-cssDir": "rm -r ../css",
    "rm-jsDir": "rm -r ../js",
    "rm-imgDir": "rm -r ../img"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "core-js": "^3.8.3",
    "js-cookie": "^3.0.1",
    "luxon": "^2.3.1",
    "mockjs": "^1.1.0",
    "vue": "^2.6.14",
    "vue-i18n": "^8.27.0",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.12",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "tailwindcss": "^3.0.23",
    "vue-template-compiler": "^2.6.14"
  }
}
